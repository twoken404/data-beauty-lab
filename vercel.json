{
  "build": {
    "env": {
      "HUGO_VERSION": "0.152.2",
      "GO_VERSION": "1.21.0"
    }
  },
  "buildCommand": "hugo --gc --minify",
  "outputDirectory": "public",
  "framework": "hugo",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "139010.xyz"
        }
      ],
      "destination": "https://139010.xyz/:path*",
      "permanent": true,
      "statusCode": 301
    }
  ]
}